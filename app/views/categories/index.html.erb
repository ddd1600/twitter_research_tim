<h1>Categories#index</h1>
<table>
		<tr>
			<th>Category Name / See Tweets</th>
			<th>&nbsp;</th>
			<th>&nbsp;</th>
			<th>&nbsp;</th>
		</tr>
	<% @categories.each do |c| %>
		<tr>
			<td><%= link_to c.title, category_url(c)%></td>
			<td><%= link_to "Edit", edit_category_url(c) %></td>
			<td><%= link_to "Delete", category_url(c), method: :delete, confirm: "Are you sure?" %></td>
		</tr>
		<% end %>
</table>
<%= link_to "Create New Category", new_category_url %>

		
